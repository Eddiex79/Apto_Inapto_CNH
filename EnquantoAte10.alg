algoritmo "conteAte10"

var
contador: inteiro

inicio
//Aqui é o inicio da contagem 0
contador<- 0
//Enquanto contador for <=10 ele vai continuar até terminar
enquanto (contador <= 10) faca
   escreval(contador)
   //Aqui ele ira adicionar + 1
   contador <- contador + 1

fimenquanto
escreval ("Terminei de contar")
fimalgoritmo

algoritmo "contRegressiva10"

var
contador: inteiro

inicio
//Aqui é o inicio da contagem 0
contador<- 10
//Enquanto contador for <=10 ele vai continuar até terminar
enquanto (contador >= 0) faca
   escreval(contador)
   //Aqui ele ira adicionar + 1
   contador <- contador - 1

fimenquanto
escreval ("Terminei de contar")
fimalgoritmo
